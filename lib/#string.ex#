defmodule StringDiff do
    

    def levenshtein(a, b) do
        levenshtein_(a, b, String.length(a), String.length(b))
    end


    def levenshtein_(_a, _b, la, lb) when la == 0 or lb == 0 do
        max(la, lb)
    end


    def levenshtein(a, b) do
        la = String.length(a)
        lb = String.length(b)
        #

        
    end

end